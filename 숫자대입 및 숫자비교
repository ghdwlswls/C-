using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication3
{
    class Program
    {
        static void Main(string[] args)
        {
            {
                Console.WriteLine("a에 숫자를 입력해주세요.");
                int a = Convert.ToInt32(Console.ReadLine());

                Console.WriteLine("b에 숫자를 입력해주세요.");
                int b = Convert.ToInt32(Console.ReadLine());

                if (a <= b)
                {
                    Console.WriteLine($"{a}는 {b}보다 작습니다.");
                }

                else if (a > b)
                {
                    Console.WriteLine($"{a}는 {b}보다 큽니다.");
                }
                    }

                }
            }

        }
    



