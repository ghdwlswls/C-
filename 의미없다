using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication3
{
    class Program
    {
        static void Main(string[] args)
        {
            {
                for(int i = 0; i <= 10; i++)
                {
                    Console.WriteLine(i);
                    {
                        if(i == 10)
                        {
                            Console.WriteLine("한계점에 도달했습니다. 더 이상 반복하지 않습니다.");
                        }
                        
                    }
                }

            }

        }
    }
}

